{{> templates/base }}
{{#partial "tituloYcss"}}
        <link rel = "stylesheet" href = "/css/Servicios.css">
        <title>Servicios</title>
 {{/partial}}
 {{> templates/base }}

 {{#partial "contenido"}}
        <div id="base">
            <h1>SERVICIOS</h1>
                <div class="row row-cols-1 g-4">
                {{#each servicios}}
                    <div class="col">
                        <div class="tarjetaServicio d-flex justify-content-between align-items-center">
                            <div>
                                <h2>{{this.nombre}}</h2>
                                <h4>Tipo: {{this.getTipo}}</h4>
                                <h4>Estado: {{this.estado}}</h4>
                            </div>
                        </div>
                    </div>
                {{/each}}
                </div>
        </div>
{{/partial}}
{{> templates/base }}